<%= form_for @user do |f| %>
  <%= f.label :username, "Username" %>
  <%= f.text_field :username , placeholder: 'Izaias' %>
  <br>
  <%= f.label :email, "Email" %>
  <%= f.text_field :email , placeholder: 'name"example.com'%>
  <br>  
  <%= f.label :password, "Password" %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit %>
<% end %>